pragma solidity 0.8.19;

import "./CREATE2Deploy.sol";


library P256Deploy {
  address internal constant VERIFIER = 0xc2b78104907F722DABAc4C69f826a522B2754De4;
  address internal constant CREATE2_FOUNDRY_FACTORY = 0x4e59b44847b379578588920cA78FbF26c0B4956C;

  bytes internal constant DATA = hex"";

  function mustDeployP256(address _vm) internal {
    if (VERIFIER.code.length > 0) {
      return;
    }

    CREATE2Deploy.mustDeployFactory(_vm);

    (bool s,) = CREATE2_FOUNDRY_FACTORY.call(DATA);
    require(s, "P256Deploy: failed to deploy P256 - call failed");
    require(VERIFIER.code.length > 0, "P256Deploy: failed to deploy P256 - no code");
  }
}
