{
  "name": "wallet-contracts",
  "version": "2.0.0",
  "private": false,
  "license": "none",
  "scripts": {
    "prepare": "npm run build",
    "build": "hardhat compile",
    "postbuild": "yarn gen:contract-typings",
    "clean": "rimraf artifacts && rimraf typings/contracts && rimraf cache",
    "pretest": "yarn build",
    "test": "hardhat test",
    "quicktest": "hardhat test",
    "precoverage": "yarn build",
    "coverage": "COVERAGE=true NET_ID=1 hardhat coverage --network coverage",
    "benchmark": "BENCHMARK=true yarn test",
    "gen:contract-typings": "rimraf typings/contracts/* && yarn gen:contract-typings:ethers4 && yarn gen:contract-typings:ethers5",
    "gen:contract-typings:ethers4": "typechain --target ethers-v4 --outDir typings/contracts/ethers-v4 './artifacts/!(build-info)/**/*[^dbg].json'",
    "gen:contract-typings:ethers5": "typechain --target ethers-v5 --outDir typings/contracts/ethers-v5 './artifacts/!(build-info)/**/*[^dbg].json'",
    "lint": "yarn lint:ts && yarn lint:sol",
    "lint:fix": "yarn lint:ts:fix && yarn lint:sol:fix",
    "lint:sol": "solhint contracts/**/*.sol",
    "lint:ts": "tslint -c tslint.json -p .",
    "lint:ts:fix": "tslint -c tslint.json -p . --fix",
    "console:ganache": "truffle console --network ganache",
    "console:rinkeby": "truffle console --network rinkeby",
    "ganache": "ganache-cli --networkId ${npm_package_config_ganacheNetworkID} --port ${npm_package_config_ganachePort} --gasLimit ${npm_package_config_ganacheGasLimit} --gasPrice ${npm_package_config_ganacheGasPrice} --defaultBalanceEther ${npm_package_config_etherBalance} --deterministic --mnemonic \"${npm_package_config_mnemonic}\" ${npm_package_config_extra}",
    "ganache:verbose": "ganache-cli --networkId ${npm_package_config_ganacheNetworkID} --verbose --port ${npm_package_config_ganachePort} --gasLimit ${npm_package_config_ganacheGasLimit} --gasPrice ${npm_package_config_ganacheGasPrice} --defaultBalanceEther ${npm_package_config_etherBalance} --deterministic --mnemonic \"${npm_package_config_mnemonic}\" ${npm_package_config_extra}",
    "ganache:stop": "ps aux | grep ganache-cli | grep -v grep | awk '{print $2}' | xargs kill -9",
    "deploy:ganache": "hardhat run --network ganache src/scripts/deploy-contracts.ts",
    "deploy": "hardhat run src/scripts/deploy-contracts.ts --network"
  },
  "files": [
    "artifacts",
    "typings"
  ],
  "config": {
    "mnemonic": "test test test test test test test test test test test junk",
    "ganacheNetworkID": 127001,
    "ganachePort": 8545,
    "ganacheGasLimit": "0xfffffffffff",
    "ganacheGasPrice": "2",
    "etherBalance": "100000",
    "extra": ""
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn lint && yarn test"
    }
  },
  "devDependencies": {
    "@arcadeum/deployer": "^0.2.4",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-truffle5": "^2.0.0",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@typechain/ethers-v4": "^4.0.0",
    "@typechain/ethers-v5": "^5.0.0",
    "@types/chai-as-promised": "^7.1.0",
    "@types/chai-string": "^1.4.1",
    "@types/mocha": "^8.2.0",
    "bn-chai": "^1.0.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-bignumber": "^3.0.0",
    "chai-string": "^1.5.0",
    "child_process": "^1.0.2",
    "dotenv": "^8.0.0",
    "ethers": "^5.0.17",
    "ganache-cli": "6.12.1",
    "hardhat": "2.0.6",
    "hardhat-gas-reporter": "1.0.4",
    "husky": "^4.2.3",
    "npm": "^6.14.8",
    "ora": "^5.1.0",
    "rimraf": "^3.0.2",
    "scrypt": "github:barrysteyn/node-scrypt#fb60a8d3c158fe115a624b5ffa7480f3a24b03fb",
    "solc": "0.7.4",
    "solhint": "^3.3.2",
    "solidity-coverage": "^0.7.13",
    "truffle": "^5.1.3",
    "truffle-contract": "^4.0.22",
    "truffle-deploy-registry": "^0.5.1",
    "truffle-hdwallet-provider": "1.0.17",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typechain": "^4.0.1",
    "typechain-target-ethers": "^1.0.3",
    "typescript": "^4.1.3",
    "yesno": "^0.3.1",
    "zos-lib": "^2.4.0"
  },
  "resolutions": {
    "sha3": "^2.0.0"
  }
}
