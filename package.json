{
  "name": "@0xsequence/wallet-contracts",
  "version": "1.3.0",
  "private": false,
  "license": "Apache-2.0",
  "scripts": {
    "build": "yarn clean && yarn compile && yarn gen:typings",
    "clean": "rimraf artifacts && rimraf typings/contracts && rimraf cache",
    "compile": "hardhat --max-memory 4096 compile",
    "gen:typings": "rimraf typings/contracts && typechain --target ethers-v5 --outDir typings/contracts './artifacts/!(build-info)/**/*[^dbg].json'",
    "test": "yarn build && hardhat test",
    "quicktest": "hardhat test",
    "precoverage": "yarn build",
    "coverage": "COVERAGE=true NET_ID=1 hardhat coverage --network coverage",
    "benchmark": "BENCHMARK=true yarn test",
    "lint": "yarn lint:ts && yarn lint:sol",
    "lint:fix": "yarn lint:ts:fix && yarn lint:sol:fix",
    "lint:sol": "solhint './contracts/**/*.sol'",
    "lint:sol:fix": "solhint './contracts/**/*.sol' --fix",
    "lint:ts": "eslint -c .eslintrc.js './**/*.ts'",
    "lint:ts:fix": "eslint -c .eslintrc.js --fix './**/*.ts'",
    "format": "prettier --write ./**/*.ts",
    "console:ganache": "truffle console --network ganache",
    "console:rinkeby": "truffle console --network rinkeby",
    "start:ganache": "ganache-cli --chainId ${npm_package_config_ganacheChainID} --networkId ${npm_package_config_ganacheChainID} --port ${npm_package_config_ganachePort} --gasLimit ${npm_package_config_ganacheGasLimit} --gasPrice ${npm_package_config_ganacheGasPrice} --defaultBalanceEther ${npm_package_config_etherBalance} --deterministic --mnemonic \"${npm_package_config_mnemonic}\" ${npm_package_config_extra}",
    "start:ganache:verbose": "yarn run start:ganache --verbose",
    "stop:ganache": "ps aux | grep ganache-cli | grep -v grep | awk '{print $2}' | xargs kill -9",
    "deploy:ganache": "hardhat run --network ganache utils/deploy-contracts.ts",
    "deploy": "hardhat run utils/deploy-contracts.ts --network",
    "verify": "hardhat verify --network"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn lint && yarn test"
    }
  },
  "devDependencies": {
    "@arcadeum/deployer": "^0.2.4",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^2.1.1",
    "@nomiclabs/hardhat-truffle5": "^2.0.0",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@typechain/ethers-v5": "^6.0.1",
    "@types/chai-as-promised": "^7.1.0",
    "@types/chai-string": "^1.4.1",
    "@types/mocha": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "bn-chai": "^1.0.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-bignumber": "^3.0.0",
    "chai-string": "^1.5.0",
    "child_process": "^1.0.2",
    "dotenv": "^8.0.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "ethers": "^5.0.31",
    "ganache-cli": "6.12.1",
    "hardhat": "2.0.6",
    "hardhat-gas-reporter": "1.0.4",
    "husky": "^4.2.3",
    "npm": "^6.14.8",
    "ora": "^5.1.0",
    "rimraf": "^3.0.2",
    "scrypt": "github:barrysteyn/node-scrypt#fb60a8d3c158fe115a624b5ffa7480f3a24b03fb",
    "solhint": "^3.3.2",
    "solidity-coverage": "^0.7.13",
    "truffle": "^5.1.3",
    "truffle-contract": "^4.0.22",
    "truffle-deploy-registry": "^0.5.1",
    "truffle-hdwallet-provider": "1.0.17",
    "ts-node": "^9.1.1",
    "typechain": "^4.0.2",
    "typechain-target-ethers": "^1.0.3",
    "typescript": "^4.1.3",
    "yesno": "^0.3.1",
    "zos-lib": "^2.4.0"
  },
  "files": [
    "LICENSE",
    "artifacts",
    "contracts",
    "networks",
    "typings"
  ],
  "resolutions": {
    "sha3": "^2.0.0"
  },
  "config": {
    "mnemonic": "test test test test test test test test test test test junk",
    "ganacheChainID": 127001,
    "ganachePort": 8545,
    "ganacheGasLimit": "0xfffffffffff",
    "ganacheGasPrice": "2",
    "etherBalance": "100000",
    "extra": ""
  }
}
